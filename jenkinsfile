pipeline{
  agent any
  stages{
    stage('Compile Stage'){
        steps{
            withMaven(maven: 'maven_3_19_0'){
                sh 'mvn clean install'
            }

        }

    }
    stage ('Cucumber Reports'){
              steps{
                  
                  cucumber bildStatus: "UNSTABLE",
                  fileIncludePattern: "**/cucu,ber.json",
                  jsonReportDirectory: 'target'
              }

          }


}
  
}
